:2px #e09850 dashed;border-radius:5px}@media (max-width: 800px){.meals{grid-template-columns:repeat(3, 1fr)}}@media (max-width: 700px){.meals{grid-template-columns:repeat(2, 1fr)}.meal{height:200px;width:200px}}@media (max-width: 500px){.meals{grid-template-columns:1fr}.meal{height:300px;width:300px}input[type='text']{width:100%}}*{box-sizing:border-box}body{background-color:#2d2013;color:#fff;font-family:Verdana, Geneva, Tahoma, sans-serif;margin:0}\n"},"lineCount":null}},"error":null,"hash":"b7fa5d058c455f76afadd84133cf9990","cacheData":{"env":{}}}